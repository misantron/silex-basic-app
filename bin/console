#!/usr/bin/env php
<?php

set_time_limit(0);

$app = require_once __DIR__ . '/../app/app.php';

$app->register(new \App\Base\Provider\Service\ConsoleServiceProvider(), [
    'console.name' => 'Application console',
    'console.version' => '1.0.0',
]);

/** @var \App\Base\Console\ConsoleApplication $console */
$console = $app['console'];
$console->run();