{
    "name": "misantron/silex-skeleton",
    "description": "Simple skeleton application on Silex framework",
    "keywords": ["php", "silex", "framework", "skeleton", "application"],
    "homepage": "http://github.com/misantron/silex-basic-app",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Ivanov",
            "email": "misantron@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "silex/silex": "~2.0",
        "misantron/silex-config-provider": "~1.0",
        "symfony/console": "~4.0",
        "symfony/event-dispatcher": "~3.0",
        "symfony/twig-bridge": "~4.0",
        "symfony/monolog-bridge": "~4.0",
        "robmorgan/phinx": "~0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "symfony/browser-kit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Application\\Tests\\": "tests/"
        }
    }
}
