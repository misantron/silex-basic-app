{
    "name": "misantron/silex-skeleton",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^5.5",
        "silex/silex": "~2.0",
        "symfony/console": "~2.8|3.0.*",
        "symfony/twig-bridge": "~2.8|3.0.*",
        "symfony/monolog-bridge": "~2.8|3.0.*",
        "doctrine/migrations": "~1.0",
        "doctrine/dbal": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
